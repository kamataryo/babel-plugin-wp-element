{
  "name": "babel-plugin-wp-element",
  "description": "A babel plugin to convert the react import declaration for WordPress.",
  "version": "0.0.2",
  "main": "dist/index.js",
  "repository": "git@github.com:kamataryo/babel-plugin-wp-element.git",
  "author": "kamataryo <mugil.cephalus+github.com@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "e2e": "./e2e/test.sh",
    "build": "tsc",
    "prepublish": "npm test && npm run build && npm run e2e"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@types/babel__core": "^7.1.2",
    "@types/node": "^12.0.10",
    "ava": "^2.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
